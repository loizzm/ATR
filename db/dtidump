CREATE TABLE IF NOT EXISTS Canis (
    name VARCHAR(255),
    small_dog_wd DECIMAL(10, 2),
    small_dog_we DECIMAL(10, 2),
    big_dog_we DECIMAL(10, 2),
    big_dog_wd DECIMAL(10, 2)
);

